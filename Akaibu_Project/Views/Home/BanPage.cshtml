@{
    ViewData["Title"] = "BanPage";
    //var panelData = ViewBag.PanelData;
}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>BanPage</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
        }

        h2 {
            color: RED;
        }

        table {
            border-collapse: collapse;
            width: 100%;
            margin-top: 20px;
        }

        th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }

        th {
            background-color: #f2f2f2;
        }

        tr:hover {
            background-color: #f5f5f5;
        }
    </style>
</head>
<body>
    
    <h2>Ban for: @Model.Nick! </h2>
    @if (true)
    {
        <p><b>Account Nick: @Model.Nick </b></p>
        <p><b>Account Reason: @Model.Bans </b></p>

    }
</body>
</html>